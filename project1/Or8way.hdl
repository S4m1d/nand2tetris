CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    // 1st line
    Or(a= in[0], b= in[1], out= or11);
    Or(a= in[2], b= in[3], out= or12);
    Or(a= in[4], b= in[5], out= or13);
    Or(a= in[6], b= in[7], out= or14);

    // 2nd line
    Or(a= or11, b= or12, out= or21);
    Or(a= or13, b= or14, out= or22);

    // final or
    Or(a= or21, b= or22, out= out);
}
